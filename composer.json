{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-exif": "*",
        "ext-ffi": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "aws/aws-php-sns-message-validator": "^1.8.0",
        "aws/aws-sdk-php": "^3.238.3",
        "donatj/phpuseragentparser": "^1.8",
        "giggsey/libphonenumber-for-php": "^8.13",
        "guzzlehttp/guzzle": "^7.5.0",
        "jcupitt/vips": "^2.4",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.5.0",
        "league/fractal": "^0.20",
        "orhanerday/open-ai": "^5.2",
        "php-open-source-saver/jwt-auth": "^2.2",
        "twilio/sdk": "^8.3.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "filp/whoops": "^2.14.5",
        "mockery/mockery": "~1.6.2",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "files": [
            "app/Helpers/GlobalHelpers.php"
        ],
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
